set nu
set hlsearch
set paste
set mouse=
set nocp
set tabstop=4
set shiftwidth=4
set history=1000
set expandtab
set noswapfile
set incsearch
set backspace=indent,eol,start
set selection=exclusive
set scrolloff=1
set autoindent
set cindent 
set cinoptions={0,1s,t0,n-2,p2s,(03s,=.5s,>1s,=1s,:1s 

map <F2>  :tabnext<CR>
map <F4>  :q<CR>
